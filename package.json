{
  "name": "@alpine-app",
  "version": "0.1.0",
  "description": "alpine app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephendltg/alpine-theme.git"
  },
  "bugs": {
    "url": "https://github.com/stephendltg/alpine-theme.git/issues"
  },
  "homepage": "https://github.com/stephendltg/alpine-theme.git#readme",
  "scripts": {
    "build": "env NODE_ENV=production rollup --config rollup.prod.config.js && postcss src/static/css/style.css -o dist/static/css/style.css",
    "watch": "rollup --config rollup.dev.config.js -w && postcss src/static/css/style.css -o dist/static/css/style.css"
  },
  "keywords": [
    "alpine",
    "spruce",
    "starter"
  ],
