  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "postcss-cli": "^8.3.0",
    "rollup": "^2.26.4",
    "rollup-plugin-copy-watch": "0.0.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0"
  }
